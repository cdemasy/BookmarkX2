{
  "manifest_version": 2,

  "name": "BookmarkX2",
  "description": "Bookmarks for your bookmarks.",
  "version": "1.0",

  "browser_action": {
   "default_icon": "icon.png",
   "default_popup": "popup.html"
  },
  "background": {
    "scripts": ["chrome-extension-async.js", "background.js"]
  },
  "permissions": [
   "activeTab",
   "bookmarks",
   "storage",
   "notifications"
   ]
}
